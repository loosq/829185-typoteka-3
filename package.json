{
  "name": "829185-typoteka-3",
  "version": "1.0.0",
  "description": "Типотека",
  "main": "./src/service/service.js",
  "scripts": {
    "start-dev-back-server": "cross-env LOG_LEVEL=debug NODE_ENV=development nodemon -r dotenv/config ./src/service/service.js --server",
    "start-dev-front-server": "cross-env LOG_LEVEL=debug NODE_ENV=development nodemon -r dotenv/config ./src/express/express.js",
    "start-servers::debug": "concurrently \"npm run start-dev-back-server\" \"npm run start-dev-front-server\"",
    "start-prod-back-server": "cross-env LOG_LEVEL=error node -r dotenv/config ./src/service/service.js > ./src/service/logs/app.log 2> ./src/service/logs/error.log --server",
    "start-prod-front-server": "cross-env LOG_LEVEL=error node -r dotenv/config ./src/service/service.js > ./src/express/logs/app.log 2> ./src/express/logs/error.log ./src/express/express.js",
    "start-servers": "concurrently \"npm run start-prod-back-server\" \"npm run start-prod-front-server\"",
    "test::rest": "jest",
    "lint": "eslint --ext .js src/",
    "lint::fix": "eslint --fix --ext .js src/",
    "filldb": "node -r dotenv/config ./src/service/service.js --filldb 10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loosq/829185-typoteka-3.git"
  },
  "author": "Polonskii Sergei",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/loosq/829185-typoteka-3/issues"
  },
  "homepage": "https://github.com/loosq/829185-typoteka-3#readme",
  "devDependencies": {
    "eslint": "7.24.0",
    "eslint-config-htmlacademy": "0.6.0",
    "jest": "27.0.6",
    "nodemon": "2.0.7",
    "sqlite3": "5.0.2",
    "supertest": "6.1.6"
  },
  "dependencies": {
    "axios": "0.21.1",
    "bcrypt": "5.0.1",
    "chalk": "4.1.0",
    "concurrently": "6.2.1",
    "connect-session-sequelize": "7.1.1",
    "cross-env": "7.0.3",
    "csurf": "1.11.0",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "express-session": "1.17.2",
    "joi": "17.4.2",
    "moment": "^2.29.1",
    "multer": "1.4.2",
    "nanoid": "3.1.22",
    "pg": "8.7.1",
    "pino": "6.13.3",
    "pino-pretty": "6.0.0",
    "pug": "3.0.2",
    "sequelize": "6.6.5"
  }
}
